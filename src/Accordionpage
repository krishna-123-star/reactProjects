        </AccordionSummary>
        <AccordionDetails>
          <Typography>This is content inside Accordion 1</Typography>
        </AccordionDetails>
      </Accordion>

      <Accordion>
        <AccordionSummary expandIcon={<ExpandMoreIcon />}>
          <Typography>Accordion 2</Typography>
        </AccordionSummary>
        <AccordionDetails>
          <Typography>This is content inside Accordion 2</Typography>
        </AccordionDetails>
      </Accordion>
    </Box>
  );
}

export default AccordionPage;
